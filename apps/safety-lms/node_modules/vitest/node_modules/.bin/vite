#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules/vite/bin/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules/vite/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules/vite/bin/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules/vite/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite@7.1.9_@types+node@20.1_08b7aa45a69e010f360fce5f2b73251a/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../vite/bin/vite.js" "$@"
fi
