#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite-node@3.2.4_@types+node_39dee9fffc31efcfaff37d0513c2f1bf/node_modules/vite-node/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite-node@3.2.4_@types+node_39dee9fffc31efcfaff37d0513c2f1bf/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite-node@3.2.4_@types+node_39dee9fffc31efcfaff37d0513c2f1bf/node_modules/vite-node/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/vite-node@3.2.4_@types+node_39dee9fffc31efcfaff37d0513c2f1bf/node_modules:/mnt/c/Users/jshepherd/OneDrive - SpecChem, LLC/Desktop/specchem-safety-lms-v2-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../vite-node/vite-node.mjs" "$@"
fi
